#at bang
compiles:
function() {
  sum = add!(3, 4)
  ok(sum == 7)
}

to:
 function() {
  add(3, 4, function(sum) {
    ok(sum == 7) })
}

compiles:
function() {
  create!()
  //x
 }

to:
function() {
  create(function() {
    //x
   }) }


compiles:
x = null
{
  create!()
  x = 10
}

to:
var x;
x = null
function() {
  create(function() {
    return x = 10
  })
}
